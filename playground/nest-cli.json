{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "projects": {
    "goerli": {
      "sourceRoot": "src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.goerli.json"
      }
    },
    "mainnet": {
      "sourceRoot": "src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.mainnet.json"
      }
    }
  }
}
