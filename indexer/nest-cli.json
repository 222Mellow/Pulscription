{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "projects": {
    "sepolia": {
      "sourceRoot": "src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.sepolia.json"
      }
    },
    "mainnet": {
      "sourceRoot": "src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.mainnet.json"
      }
    }
  }
}
